[Unit]
Description=ZIVPN Enhanced Services Manager
After=network.target zivpn.service zivpn-monitor.service
Wants=zivpn.service zivpn-monitor.service

[Service]
Type=oneshot
User=root
WorkingDirectory=/etc/zivpn
ExecStart=/bin/echo "ZIVPN Enhanced Services are running"
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
